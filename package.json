{
    "name": "socketfox",
    "productName": "Socketfox",
    "version": "0.6.1",
    "license": "GPL-3.0-only",
    "author": {
        "name": "Richard Kriesman",
        "email": "rkriesman@gmail.com",
        "url": "https://richardkriesman.com"
    },
    "build": {
        "appId": "com.richardkriesman.socketfox",
        "appImage": {
            "category": "Development",
            "synopsis": "Develop better, faster websockets in Socket.IO."
        },
        "directories": {
            "output": "build"
        },
        "linux": {
            "icon": "build/icon.icns",
            "publish": {
                "provider": "generic",
                "url": "https://opensource.room236.com/socketfox",
                "channel": "latest"
            },
            "target": "AppImage"
        },
        "mac": {
            "publish": {
                "provider": "generic",
                "url": "https://opensource.room236.com/socketfox",
                "channel": "latest"
            },
            "target": "dmg"
        },
        "win": {
            "publish": {
                "provider": "generic",
                "url": "https://opensource.room236.com/socketfox",
                "channel": "latest"
            },
            "target": "nsis"
        }
    },
    "private": true,
    "main": "app/dist/main/main.js",
    "scripts": {
        "build": "yarn run-script build",
        "run-script": "chmod +x scripts/run-script.sh && scripts/run-script.sh",
        "start": "electron ."
    },
    "devDependencies": {
        "@types/ace": "^0.0.40",
        "@types/electron-is-dev": "^0.3.0",
        "@types/jquery": "^3.3.4",
        "@types/jqueryui": "^1.12.2",
        "@types/socket.io-client": "^1.4.32",
        "electron": "^2.0.8",
        "electron-builder": "^20.26.0",
        "electron-packager": "^12.1.0",
        "sass": "^1.10.1",
        "tslint": "^5.10.0",
        "typescript": "^2.9.2"
    },
    "dependencies": {
        "ace-builds": "^1.3.3",
        "debug": "^3.1.0",
        "electron-about-window": "^1.12.1",
        "electron-is-dev": "^0.3.0",
        "electron-updater": "^3.0.3",
        "jquery": "^3.3.1",
        "jqueryui": "^1.11.1",
        "socket.io-client": "^2.1.1",
        "socketio-wildcard": "^2.0.0"
    }
}
